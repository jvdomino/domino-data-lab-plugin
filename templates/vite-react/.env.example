# Domino React App Environment Variables
# Copy this file to .env.local for local development

# Model API Endpoint
# Get this URL from Domino Model APIs page
VITE_MODEL_API_URL=https://your-domino.company.com/models/MODEL_ID/latest/model

# Model API Authentication Token
# Generate from Domino > Account Settings > API Keys
VITE_MODEL_API_TOKEN=your_api_token_here

# Optional: Additional configuration
# VITE_APP_TITLE=My Domino App
# VITE_DEBUG=false
